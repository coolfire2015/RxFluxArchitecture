<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.huyingbao.module.wan.kotlin">

    <uses-permission android:name="android.permission.INTERNET" />

    <!--一定要使用明文通信-->
    <application
        android:name="com.huyingbao.module.wan.WanApplication"
        android:allowBackup="false"
        android:icon="@drawable/ic_main"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_main"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.huyingbao.module.wan.ui.article.view.ArticleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.huyingbao.module.wan.ui.login.view.LoginActivity" />
    </application>
</manifest>