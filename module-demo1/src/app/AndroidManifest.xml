<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.huyingbao.module.first">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.huyingbao.module.first.FirstApplication"
        android:allowBackup="false"
        android:icon="@drawable/ic_main"
        android:label="@string/app_label_first"
        android:roundIcon="@drawable/ic_main"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.huyingbao.module.first.ui.view.FirstActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>